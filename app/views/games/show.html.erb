<%= content_for(:title, @game.title) %>
<div class="profile col-md-6">
<% cache @game do %>
<%= render 'profile' %>
<% end %>
</div>

<div class="feed col-md-6">
<h3>Check-ins</h3>

<div id="posts">
	<%= render @posts %>
</div>

<div id="infinite-scrolling">
	<%= will_paginate @posts %>
</div>

</div>
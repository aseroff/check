<%= content_for(:title, @user.username) %>

<div class="profile col-sm-4 contain">
<% #cache @user do %>
<%= render 'profile' %>
<% #end %>
<%= render 'controls' %>
</div>

<div class="feed col-sm-8">  
<h3 class="col-sm-12">Check-ins</h3>

<div id="posts">
<%= render @posts %>
</div>

<div id="infinite-scrolling">
	<%= will_paginate @posts %>
</div>
</div>
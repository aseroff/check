<%= content_for(:title, @user.username + "'s followers") %>
<div class="col-sm-4">

<div class="contain profile">
<%= render 'profile' %>
<%= render 'controls' %>
</div>

</div>

<div class="feed col-sm-8">  
<h3 class="col-sm-12">Followers</h3>

<div id="user-list" class="col-sm-12">
<%= render @users %>
</div>

<div id="infinite-scrolling">
	<%= will_paginate @users %>
</div>
</div>